[
{
      "text": "一期一会",
      "author": "Traditional"
    },
    {
      "text": "七転び八起き",
      "author": "Traditional"
    },
    {
      "text": "和を以て貴しと為す",
      "author": "Seventeen-Article Constitution"
    },
    {
      "text": "花は桜木人は武士",
      "author": "Traditional"
    },
    {
      "text": "石の上にも三年",
      "author": "Traditional"
    },
    {
      "text": "猿も木から落ちる",
      "author": "Traditional"
    },
    {
      "text": "光陰矢の如し",
      "author": "Traditional"
    },
    {
      "text": "鳴かぬなら鳴かせてみせよう鴬",
      "author": "Tokugawa Ieyasu"
    },
    {
      "text": "出る杭は打たれる",
      "author": "Traditional"
    },
    {
      "text": "千里の道も一歩から",
      "author": "Laozi (Chinese origin)"
    },
    {
      "text": "武士道と云ふは死ぬ事と見付けたり",
      "author": "Yamamoto Tsunetomo"
    },
    {
      "text": "夢を持ち続ける",
      "author": "Hayao Miyazaki"
    },
    {
      "text": "人生意気に感ず",
      "author": "Traditional"
    },
    {
      "text": "雨降って地固まる",
      "author": "Traditional"
    },
    {
      "text": "起きて半畳寝て一畳",
      "author": "Traditional"
    },
    {
      "text": "急がば回れ",
      "author": "Traditional"
    },
    {
      "text": "蓼食う虫も好き好き",
      "author": "Traditional"
    },
    {
      "text": "備えあれば憂いなし",
      "author": "Traditional"
    },
    {
      "text": "虎穴に入らずんば虎子を得ず",
      "author": "Traditional"
    },
    {
      "text": "果報は寝て待て",
      "author": "Traditional"
    },
    {
      "text": "一を聞いて十を知る",
      "author": "Traditional"
    },
    {
      "text": "月下の一群",
      "author": "Traditional"
    },
    {
      "text": "濡れ手で粟",
      "author": "Traditional"
    },
    {
      "text": "二兎を追う者は一兎をも得ず",
      "author": "Traditional"
    },
    {
      "text": "塵も積もれば山となる",
      "author": "Traditional"
    },
    {
      "text": "井の中の蛙大海を知らず",
      "author": "Traditional"
    },
    {
      "text": "大器晩成",
      "author": "Traditional"
    },
    {
      "text": "天は自ら助くる者を助く",
      "author": "Traditional"
    },
    {
      "text": "人生は短し、芸術は長し",
      "author": "Hippocrates (Greek origin)"
    },
    {
      "text": "石の上にも三年",
      "author": "Traditional"
    }
  ]
